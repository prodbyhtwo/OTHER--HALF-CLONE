# Example Environment Variables
# Copy this file to .env.local and fill in actual values

# Builder.io (optional)
VITE_PUBLIC_BUILDER_KEY=__BUILDER_PUBLIC_KEY__

# Development
NODE_ENV=development
LOG_LEVEL=debug

# Database
DATABASE_URL=postgresql://username:password@localhost:5432/datify_dev
DATABASE_READ_REPLICA_URL=postgresql://username:password@localhost:5432/datify_dev

# Authentication & Security
JWT_SECRET=__GENERATE_RANDOM_SECRET__
ENCRYPTION_KEY=__GENERATE_32_CHAR_KEY__
SESSION_SECRET=__GENERATE_RANDOM_SECRET__

# Storage (AWS S3 or compatible)
STORAGE_BUCKET_NAME=datify-uploads-dev
STORAGE_BUCKET_REGION=us-east-1
STORAGE_ACCESS_KEY=__AWS_ACCESS_KEY__
STORAGE_SECRET_KEY=__AWS_SECRET_KEY__

# Stripe (use test keys in development)
STRIPE_PUBLISHABLE_KEY=pk_test_...
STRIPE_SECRET_KEY=sk_test_...
STRIPE_WEBHOOK_SECRET=whsec_...

# Google APIs
GOOGLE_PLACES_API_KEY=__YOUR_GOOGLE_PLACES_KEY__
GOOGLE_GEOCODING_API_KEY=__YOUR_GOOGLE_GEOCODING_KEY__

# Email
SENDGRID_API_KEY=__YOUR_SENDGRID_KEY__

# Push Notifications
PUSH_NOTIFICATION_SERVER_KEY=__YOUR_FCM_SERVER_KEY__

# Monitoring
SENTRY_DSN=__YOUR_SENTRY_DSN__
ANALYTICS_API_KEY=__YOUR_ANALYTICS_KEY__

# Testing/CI
CI=false
PING_MESSAGE="ping pong"
